{
    "name": "playwright-cucumber-sample-project",
    "version": "1.0.0",
    "description": "Sample project with Playwright and Cucumber BDD",
    "main": "index.js",
    "scripts": {
        "test": "npx cucumber-js -p runner & npx ts-node ./src/support/reporter/HTMLReporter.ts",
        "test:tags": "npx cucumber-js -p runner --tags",
        "failed:test": "npx cucumber-js -p rerun",
        "report": "npx ts-node ./src/support/reporter/HTMLReporter.ts",
        "test:staging": "ENVIRONMENT=staging npm run test:tags",
        "test:production": "ENVIRONMENT=production npm run test:tags",
        "test:dynamic": "ENVIRONMENT=dynamic npm run test:tags",
        "test:local": "ENVIRONMENT=local npm run test:tags",
        "lint": "eslint .",
        "lint:fix": "npm run lint -- --fix"
    },
    "keywords": [],
    "devDependencies": {
        "@cucumber/cucumber": "8.7.0",
        "@cucumber/pretty-formatter": "1.0.0",
        "@playwright/test": "1.29.0",
        "@types/cucumber-html-reporter": "5.0.1",
        "@types/fs-extra": "^9.0.13",
        "@types/randomstring": "^1.1.8",
        "@types/string-format": "^2.0.0",
        "@types/xmldom": "^0.1.31",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "cross-env": "7.0.3",
        "copyfiles": "^2.4.1",
        "dotenv": "16.0.0",
        "eslint": "^8.24.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^3.4.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-no-relative-import-paths": "^1.4.0",
        "eslint-plugin-playwright": "^0.11.2",
        "eslint-plugin-prettier": "^4.2.1",
        "multiple-cucumber-html-reporter": "3.0.1",
        "os": "0.1.2",
        "playwright": "1.29.0",
        "randomstring": "1.2.2",
        "string-format": "2.0.0",
        "ts-node": "10.9.1",
        "typescript": "4.8.4",
        "winston": "3.8.2",
        "xml-formatter": "2.6.1",
        "xmldom": "0.6.0",
        "xpath": "0.0.32"
    },
    "dependencies": {
        "jwt-decode": "^3.1.2",
        "moment": "^2.29.4"
    }
}
