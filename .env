# Browser type chrome, firefox & webkit
BROWSER=chrome

# Launch browser when headless is set to false
HEADLESS=false

# Viewport width and height
VIEWPORT_WIDTH=1280
VIEWPORT_HEIGHT=720

# These timeouts are set in minutes
TEST_TIMEOUT=20
BROWSER_LAUNCH_TIMEOUT=0
WAIT_TIMEOUT=1

# Execution configurations
RETRIES=1
PARALLEL_THREAD=6

# Test application configurations
ENVIRONMENT= staging
DYNAMIC_SUBDOMAIN=

#production, staging, dynamic environment
BASE_URL=https://demo.playwright.dev/todomvc/


RECORD_VIDEO=true
